{
  "manifest_version": 2,
  "name": "Find my meeting room",
  "version": "1.0",
  "description": "Find the meeting room by providing the meeting room ID",
  "browser_action": {
    "default_icon": "building.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}